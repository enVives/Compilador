                            //******************Intermedi******************
                                taula_intermedi.genera(Operacio.copia,i.getR(),null,r.getR()); //r.getR() += I
                            //********************************************
                            RESULT = new SimbolModifTipus(r.getTsb(),r.getTipus(),r.getEsquerre(),i.getDreta(),r.getR());
                        }
